{
  "include": [
    "custom_components"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "recipecards-card"
  ],
  "ignore": [
    "**/node_modules",
    "**/__pycache__"
  ],
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "reportUnusedImport": false,
  "reportUnusedClass": false,
  "reportUnusedFunction": false,
  "reportUnusedVariable": false,
  "pythonVersion": "3.10",
  "typeCheckingMode": "basic"
} 